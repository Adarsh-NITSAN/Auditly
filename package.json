{
  "name": "automatic-audit",
  "version": "1.0.0",
  "description": "Automatic accessibility audit tool with crawling and reporting capabilities",
  "main": "dist/server/server.js",
  "type": "module",
  "scripts": {
    "dev": "npm run build:server && concurrently \"npm:dev:server\" \"npm:dev:frontend\"",
    "dev:server": "nodemon dist/server/server.js",
    "dev:frontend": "vite",
    "build": "npm run build:server && npm run build:frontend",
    "build:server": "tsc -p tsconfig.server.json",
    "build:frontend": "vite build",
    "start": "node dist/server/server.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "accessibility",
    "audit",
    "crawler",
    "a11y",
    "wcag",
    "vite",
    "typescript"
  ],
  "author": "Accesstive Team",
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.7",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/url-parse": "^1.4.11",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
